    node_include                 longtext     null,
    retry_prev_id                bigint       null,
    success_on_empty_node_filter bit          null,
    extra_metadata               longtext     null,
    node_exclude_os_version      longtext     null,
    timeout                      longtext     null,
    node_exclude_precedence      bit          null,
    node_exclude_name            longtext     null,
    node_include_os_version      longtext     null,
    node_exclude_os_name         longtext     null,
    retry                        longtext     null,
    filter                       longtext     null,
    orchestrator_id              bigint       null,
    node_include_name            longtext     null,
    rduser                       varchar(255) not null,
    retry_original_id            bigint       null,
    execution_type               varchar(30)  null,
