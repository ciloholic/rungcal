    day_of_month                  varchar(255)  null,
    node_filter_editable          bit           null,
    log_output_threshold_status   varchar(256)  null,
    description                   longtext      null,
    minute                        varchar(255)  null,
    project                       varchar(255)  not null,
    plugin_config                 longtext      null
);

